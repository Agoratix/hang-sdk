{
  "increment": "conventional:angular",
  "changelogCommand": "conventional-changelog -p angular | tail -n +3",
  "safeBump": false,
  "npm": {
    "publish": true
  },
  "git": {
    "commitMessage": "chore(release): cut the v${version} release",
    "tagName": "v${version}"
  },
  "github": {
    "release": true,
    "releaseName": "hang-sdk v${version} Released!",
    "draft": true
  }
}
