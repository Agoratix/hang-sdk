{
  "version": "0.10.6",
  "source": "src/index.ts",
  "license": "MIT",
  "main": "dist/main.js",
  "module": "./dist/module.js",
  "types": "./dist/types.d.ts",
  "repository": "https://github.com/Agoratix/hang-sdk",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "start": "parcel watch",
    "build": "parcel build --no-scope-hoist",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "yarn build && husky install",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "release": "release-it --ci"
  },
  "targets": {
    "global": {
      "context": "browser",
      "outputFormat": "global"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "hang-sdk",
  "author": "Azim Ahmed",
  "size-limit": [
    {
      "path": "dist/hang-sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/hang-sdk.esm.js",
      "limit": "10 KB"
    }
  ],
  "keywords": [
    "web3",
    "hang",
    "sdk"
  ],
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "dependencies": {
    "@coinbase/wallet-sdk": "^3.3.0",
    "@crossmint/client-sdk-base": "^0.1.2-alpha.2",
    "@parcel/transformer-typescript-tsc": "^2.5.0",
    "@walletconnect/web3-provider": "^1.7.7",
    "bignumber.js": "^9.0.2",
    "eslint-plugin-prettier": "3.4.1",
    "keccak256": "^1.0.6",
    "merkletreejs": "^0.2.27",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-is": "^18.0.0",
    "tslib": "^2.4.0",
    "web3": "^1.7.1",
    "web3modal": "^1.9.6"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@commitlint/config-conventional": "^16.2.1",
    "@parcel/packager-ts": "2.5.0",
    "@parcel/transformer-typescript-types": "2.5.0",
    "@release-it/conventional-changelog": "^4.3.0",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@storybook/addon-essentials": "^6.4.20",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.4.20",
    "@storybook/addons": "^6.4.20",
    "@storybook/react": "^6.4.20",
    "@types/events": "^3.0.0",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "babel-loader": "^8.2.4",
    "commitlint": "^16.2.3",
    "husky": "^7.0.0",
    "parcel": "^2.5.0",
    "release-it": "^14.14.2",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "typescript": "^4.6.3"
  }
}
